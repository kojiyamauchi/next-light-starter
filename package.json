  "scripts": {
    "prepare": "husky install",
    "setup": "rm -rf .git",
    "dev:app": "NODE_OPTIONS='--inspect' next dev",
    "dev:storybook": "BROWSER='google chrome canary' start-storybook -s ./public -p 6006",
    "dev:webp": "chokidar --silent true 'public/webps/*' -c 'rimraf public/webps/*.{png,jpg,jpeg,gif}'",
    "dev:path": "pathpida -o ./src/types/ -w",
    "dev": "run-p dev:*",
    "build:app": "next build && rimraf public/webps/*.{png,jpg,jpeg,gif} && next export -o delivery/",
    "build:storybook": "build-storybook -s ./public -o delivery/storybook",
    "build:path": "pathpida -o ./src/types/",
    "build": "run-p build:storybook",
    "serve": "next start",
    "clean": "rimraf delivery/* && touch delivery/.gitkeep",
    "jest": "jest --passWithNoTests",
    "add:fc": "hygen add component",
    "add:page": "hygen add page",
    "add:hooks": "hygen add hooks",
    "add:store": "hygen add store",
    "ui": "yarn upgrade-interactive --latest"
  }
